    },
    "VsoCorefxGitUrl": {
      "value": "https://$(VsoAccountName):$(VsoPassword)@devdiv.visualstudio.com/DevDiv/_git/$(VsoRepositoryName)/"
    }
  },
  "demands": [
    "Agent.OS -equals darwin"
