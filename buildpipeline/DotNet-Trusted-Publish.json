      "task": {
        "id": "e213ff0f-5d5c-4791-802d-52ea3e7be1f1",
        "versionSpec": "1.*",
        "definitionType": "task"
      },
      "inputs": {
        "scriptType": "filePath",
        "scriptName": "scripts/DotNet-Trusted-Publish/Fetch-Tools.ps1",
        "arguments": "$(Build.StagingDirectory)\\ToolingDownload",
